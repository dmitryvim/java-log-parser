<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-configuration PUBLIC
        "-//Hibernate/Hibernate Configuration DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-configuration-3.0.dtd">
<hibernate-configuration>
    <session-factory>
        <property name="hibernate.connection.driver_class">com.mysql.jdbc.Driver</property>
        <property name="hibernate.connection.username">parser</property>
        <property name="hibernate.connection.password">parser</property>
        <property name="hibernate.connection.url">jdbc:mysql://localhost:3306/log_parser</property>

        <mapping class="com.ef.Log"/>

    </session-factory>
</hibernate-configuration>